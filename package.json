{
  "name": "chess-algos",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/js/jsdomSetup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/legacy_notes_and_workbook/",
      "<rootDir>/client/src/stylesheets/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^1.9.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.27.3",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "jest": "^22.4.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "reactify": "^1.1.0",
    "style-loader": "^0.13.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.24.0",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "chess.js": "^0.10.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.3",
    "flexboxgrid2": "^7.2.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "postcss-loader": "^2.0.1",
    "react-bootstrap": "0.32.1",
    "react-bootstrap-table": "3.1.6",
    "react-chessdiagram": "^1.1.8",
    "react-d3-basic": "1.6.11",
    "react-d3-components": "0.7.1",
    "react-flexbox-grid": "^2.0.0",
    "react-select": "^1.1.0",
    "react-toastr": "^2.8.2",
    "style-loader": "^0.13.2"
  }
